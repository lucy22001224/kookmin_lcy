int red = 7;

void setup() 
{
  pinMode(red, OUTPUT);
  delay(1000);
  digitalWrite(red, 1);
  delay(100);
  digitalWrite(red, 0);
  delay(100);
  digitalWrite(red, 1);
  delay(100);
  digitalWrite(red, 0);
  delay(100);
  digitalWrite(red, 1);
  delay(100);
  digitalWrite(red, 0);
  delay(100);
  digitalWrite(red, 1);
  delay(100);
  digitalWrite(red, 0);
  delay(100);
  digitalWrite(red, 1);
  delay(100);
  digitalWrite(red, 0);
  delay(100);
  digitalWrite(red, 1);
}

void loop() 
{
  while(1){}
}
